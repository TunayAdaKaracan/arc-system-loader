--[[pod_format="raw",created="2024-03-06 14:24:10",modified="2024-03-12 17:25:44",revision=374]]
-- Modern Monster
-- by ocojo

rare={35,42,56}
halloween={46,47,48,49,50}
xmas={59,60,61}
easter={62,63,64}

bt = 0
freq = 100

function _init()
	local g0 = {}
	for i=1,64 do add(g0,i) end
	foreach(rare, function(e) del(g0, e) end)
	foreach(halloween, function(e) del(g0, e) end)
	foreach(xmas, function(e) del(g0, e) end)
	foreach(easter, function(e) del(g0, e) end)
	
	mon = {}
	
	-- add from regular collection
	for i=1,11 do
		del(g0, add(mon, rnd(g0)))
	end
	
	-- rare
	if (rnd(20)<1) then
		local index = 1+rnd(11)\1
		mon[index] = rnd(rare)
	end
	
	-- halloween
	if (date():sub(6,10)=="2024-10-31") then
		local index = 1+rnd(11)\1
		mon[index] = del(halloween, rnd(halloween))
	end
	
	-- easter
	if date():sub(1,10)=="2024-03-06" then
		for i=1,2 do
			local index = 1+rnd(11)\1
			mon[index] = del(easter, rnd(easter))
		end
	end
	
	-- xmas
	if (date():sub(6,10)=="2024-10-31") then
		for i=1,2 do
			local index = 1+rnd(11)\1
			mon[index] = del(xmas, rnd(xmas))
		end
	end
	
	for i=1,#mon do
		mon[i] = {
			index = mon[i],
			blink = rnd(freq)\1
		}
	end
	
end


function _draw()
	cls(theme"desktop0")
	
	
	i = 1
	for y=0,2 do
		for x=0, 3-(y%2) do
	
			local sx = 20+x * 120 + (y%2)*60
			local sy = 30+y*70
			
			pal(1,theme"desktop1")
			
			
			bt += 1/120
			if (bt>=freq) then
				bt = 0
				for i=1,#mon do
					mon[i].blink = rnd(freq)\1
				end
			end
			q = flr(bt)
			
			if mon[i].blink == q  then
				spr(mon[i].index*2+1,sx,sy)
			else
				spr(mon[i].index*2+0,sx,sy)
			end
			
			i+=1
		end
	end
end

function _update()
	if (rnd(120)<1) blinky = (rnd(12)\1)*2
end

